{
  "expo": {
    "name": "Play in the Sandbox Looking at Flowers",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "playsandbox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.djscottyb.playsandbox",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let you take profile pictures.",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "playsandbox"
            ]
          }
        ],
        "NSUserTrackingUsageDescription": "This allows us to provide a better app experience by understanding how you use the app.",
        "UIRequiresFullScreen": true,
        "LSApplicationQueriesSchemes": [
          "https",
          "http"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "requireFullScreen": true,
      "associatedDomains": [],
      "appStoreUrl": "https://apps.apple.com/app/id6477183822",
      "privacyManifestUrl": "https://your-domain.com/privacy-manifest.json",
      "accessibilityInfo": {
        "voiceOverEnabled": true,
        "dynamicTypeEnabled": true
      }
    },
    "android": {
      "package": "com.djscottyb.playsandbox",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1eb49bf1-1ea0-490d-8ebc-992e30972908"
      }
    },
    "owner": "djscottyb",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "privacyPolicy": "https://your-domain.com/privacy-policy",
    "termsOfService": "https://your-domain.com/terms",
    "supportUrl": "https://your-domain.com/support",
    "appStoreConfig": {
      "categories": ["Entertainment", "Games"],
      "keywords": "sandbox, flowers, play, relaxation, garden",
      "ageRating": "4+",
      "copyright": "Â© 2024 Scott Beilfuss"
    }
  }
}
